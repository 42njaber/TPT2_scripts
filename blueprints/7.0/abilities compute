:global double dank_sacrifice_cd
:global double dispel_cd
:global double super_multishot_cd
:global double immortality_shield_cd
:global double immortality_shield_duration
:global double temporal_barrier_cd
:global double temporal_barrier_duration
:global double hurricane_cd
:global double super_tower_2_cd
:global double super_tower_2_duration
:global double super_tower_1_3_cd
:global double super_tower_1_3_duration
:global double round_delta
:global int round_going

dank_sacrifice_cd = max(i2d(round_going) * (dank_sacrifice_cd - round_delta), 0.)
dispel_cd = max(i2d(round_going) * (dispel_cd - round_delta), 0.)
super_multishot_cd = max(i2d(round_going) * (super_multishot_cd - round_delta), 0.)
immortality_shield_cd = max(i2d(round_going) * (immortality_shield_cd - round_delta), 0.)
immortality_shield_duration = max(i2d(round_going) * (immortality_shield_duration - round_delta), 0.)
temporal_barrier_cd = max(i2d(round_going) * (temporal_barrier_cd - round_delta), 0.)
temporal_barrier_duration = max(i2d(round_going) * (temporal_barrier_duration - round_delta), 0.)
hurricane_cd = max(i2d(round_going) * (hurricane_cd - round_delta), 0.)
super_tower_2_cd = max(i2d(round_going) * (super_tower_2_cd - round_delta), 0.)
super_tower_2_duration = max(i2d(round_going) * (super_tower_2_duration - round_delta), 0.)
super_tower_1_3_cd = max(i2d(round_going) * (super_tower_1_3_cd - round_delta), 0.)
super_tower_1_3_duration = max(i2d(round_going) * (super_tower_1_3_duration - round_delta), 0.)
wait(0.0)
